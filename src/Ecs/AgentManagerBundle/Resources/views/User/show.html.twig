<ul class="record_actions">
    <li>
        <a href="{{ path('agentmanager') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('agentmanager_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('agentmanager_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
