<!doctype html>
<html lang="en-us">
<head>
	<meta charset="utf-8">

	<title>Emmediate Credit Solutions | Login</title>
	
	<meta name="description" content="">
	<meta name="author" content="revaxarts.com and Simple Web Logic">


	<!-- Apple iOS and Android stuff -->
	<meta name="apple-mobile-web-app-capable" content="yes">
	<meta name="apple-mobile-web-app-status-bar-style" content="black">
	<link rel="apple-touch-icon-precomposed" href="{{ asset('img/icon.png') }}">
	<link rel="apple-touch-startup-image" href="{{ asset('img/startup.png') }}">
	<meta name="viewport" content="width=device-width,initial-scale=1,user-scalable=no,maximum-scale=1">

	<!-- Google Font and style definitions -->
	<link rel="stylesheet" href="http://fonts.googleapis.com/css?family=PT+Sans:regular,bold">
	<link rel="stylesheet" href="{{ asset('css/style.css') }}">

	<!-- include the skins (change to dark if you like) -->
	<link rel="stylesheet" href="{{ asset('css/light/theme.css') }}" id="themestyle">
	<!-- <link rel="stylesheet" href="{{ asset('css/dark/theme.css')}}" id="themestyle"> -->

	<!--[if lt IE 9]>
	<script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
	<link rel="stylesheet" href="{{ asset('css/ie.css') }}">
	<![endif]-->

	<!-- Use Google CDN for jQuery and jQuery UI -->
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.6.1/jquery.min.js"></script>
	<script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.8.12/jquery-ui.min.js"></script>

	<!-- Loading JS Files this way is not recommended! Merge them but keep their order -->

	<!-- some basic functions -->
	<script src="{{ asset('js/functions.js')}}"></script>

	<!-- all Third Party Plugins -->
	<script src="{{ asset('js/plugins.js')}}"></script>

	<!-- Whitelabel Plugins -->
	<script src="{{ asset('js/wl_Alert.js')}}"></script>
	<script src="{{ asset('js/wl_Dialog.js')}}"></script>
	<script src="{{ asset('js/wl_Form.js')}}"></script>

	<!-- configuration to overwrite settings -->
	<script src="{{ asset('js/config.js')}}"></script>

	<!-- the script which handles all the access to plugins etc... -->
	<script src="{{ asset('js/login.js')}}"></script>
</head>
<body id="login">
	<header>
		<div id="logo">
			<a href="#">ECS</a>
		</div>
	</header>

	<section id="content">
		{% for key, message in app.session.getFlashes() %}
        <div class="{{ key }}">
            {{ message|trans({}, 'FOSUserBundle') }}
        </div>
        {% endfor %}
        {% block fos_user_content %}
        {% endblock fos_user_content %}
	</section>

	<footer>Copyright by EmmediateCreditSolutions.com 2011</footer>

</body>
</html>